    <item>
      <title>数字的排列</title>
      <link>https://lyr-2000.github.io/post/11.%E4%B8%AA%E4%BA%BA%E6%80%BB%E7%BB%93/%E5%89%91%E6%8C%87offer/%E6%90%9C%E7%B4%A2/%E6%95%B0%E5%AD%97%E7%9A%84%E6%8E%92%E5%88%97%E9%87%8D%E5%A4%8D%E5%85%83%E7%B4%A0/</link>
